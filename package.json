{
  "name": "gdlauncher",
  "productName": "GDLauncher",
  "private": true,
  "description": "GDLauncher Carbon.",
  "author": "killpowa <davide@gdlauncher.com>",
  "license": "ARR",
  "scripts": {
    "init": "npm i -g pnpm@9.13.2 && npm i -g cross-env && cross-env-shell PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=true pnpm i",
    "sudo-init": "sudo npm i -g pnpm@9.13.2 && PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=true pnpm i",
    "codegen": "node iridium_patchfix.js && pnpm prisma:generate && pnpm rspc-bindings:generate",
    "watch:app": "turbo run dev --env-mode=loose --filter !@gd/website",
    "watch:core": "cargo run -p carbon_app",
    "------divider-prepare------": "------divider------",
    "prepare-release:linux-x64": "cd target/x86_64-unknown-linux-gnu/release && cp ./carbon_app ./carbon_app.debug && strip -S ./carbon_app",
    "prepare-release:mac-x64": "cd target/x86_64-apple-darwin/release && dsymutil ./carbon_app && strip -S ./carbon_app",
    "prepare-release:mac-arm64": "cd target/aarch64-apple-darwin/release && dsymutil ./carbon_app && strip -S ./carbon_app",
    "prepare-release:linux-x64-prod": "cd target/x86_64-unknown-linux-gnu/production && cp ./carbon_app ./carbon_app.debug && strip -S ./carbon_app",
    "prepare-release:mac-x64-prod": "cd target/x86_64-apple-darwin/production && dsymutil ./carbon_app && strip -S ./carbon_app",
    "prepare-release:mac-arm64-prod": "cd target/aarch64-apple-darwin/production && dsymutil ./carbon_app && strip -S ./carbon_app",
    "------divider-release------": "------divider------",
    "build:linux-x64": "cargo build -p carbon_app --release --target x86_64-unknown-linux-gnu && pnpm prepare-release:linux-x64 && turbo run build --env-mode=loose --filter !@gd/website -- --x64 --linux -- --release",
    "build:win-x64": "cargo build -p carbon_app --release --target x86_64-pc-windows-msvc && turbo run build --env-mode=loose --filter !@gd/website -- --x64 --win -- --release",
    "build:mac-universal": "cargo build -p carbon_app --release --target aarch64-apple-darwin && pnpm prepare-release:mac-arm64 && cargo build -p carbon_app --release --target x86_64-apple-darwin && pnpm prepare-release:mac-x64 && turbo run build --env-mode=loose --filter !@gd/website -- --universal --mac -- --release",
    "------divider-prod------": "------divider------",
    "build:linux-x64-prod": "cargo build -p carbon_app --profile=production --features production --target x86_64-unknown-linux-gnu && pnpm prepare-release:linux-x64-prod && turbo run build --env-mode=loose --filter !@gd/website -- --x64 --linux -- --production",
    "build:win-x64-prod": "cargo build -p carbon_app --profile=production --features production --target x86_64-pc-windows-msvc && turbo run build --env-mode=loose --filter !@gd/website -- --x64 --win -- --production",
    "build:mac-universal-prod": "cargo build -p carbon_app --profile=production --features production --target x86_64-apple-darwin && pnpm prepare-release:mac-x64-prod && cargo build -p carbon_app --profile=production --features production --target aarch64-apple-darwin && pnpm prepare-release:mac-arm64-prod && turbo run build --env-mode=loose --filter !@gd/website -- --universal --mac -- --production",
    "------divider-website----": "------divider------",
    "dev:website": "turbo run dev --env-mode=loose --filter @gd/website",
    "build:website": "turbo run build --env-mode=loose --filter @gd/website",
    "------divider-utils------": "------divider------",
    "lint": "turbo run lint --env-mode=loose --filter @gd/desktop --filter @gd/ui",
    "lint-fix": "turbo run lint-fix --env-mode=loose --filter @gd/desktop --filter @gd/ui",
    "test": "turbo run test --env-mode=loose",
    "test-prod": "turbo run test --env-mode=loose --filter @gd/desktop",
    "rspc-bindings:generate": "cargo run -p carbon_app -- --generate-ts-bindings",
    "------divider-prisma-generate-normal------": "------divider------",
    "prisma:generate": "cargo run -p prisma-cli -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:generate:linux-x64": "cargo run -p prisma-cli --release --target x86_64-unknown-linux-gnu -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:generate:mac-x64": "cargo run -p prisma-cli --release --target x86_64-apple-darwin -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:generate:mac-arm64": "cargo run -p prisma-cli --release --target aarch64-apple-darwin -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:generate:win-x64": "cargo run -p prisma-cli --release --target x86_64-pc-windows-msvc -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "------divider-prisma-generate-prod------": "------divider------",
    "prisma:generate:linux-x64-prod": "cargo run -p prisma-cli --profile=production --target x86_64-unknown-linux-gnu -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:generate:mac-x64-prod": "cargo run -p prisma-cli --profile=production --target x86_64-apple-darwin -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:generate:mac-arm64-prod": "cargo run -p prisma-cli --profile=production --target aarch64-apple-darwin -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:generate:win-x64-prod": "cargo run -p prisma-cli --profile=production --target x86_64-pc-windows-msvc -- generate --schema=./crates/carbon_repos/prisma/schema.prisma",
    "------divider-prisma-utils------": "------divider------",
    "prisma:studio": "npx prisma studio --schema ./crates/carbon_repos/prisma/schema.prisma",
    "prisma:migrate": "cargo run -p prisma-cli --release -- migrate dev --schema=./crates/carbon_repos/prisma/schema.prisma",
    "prisma:reset": "cargo run -p prisma-cli --release -- migrate reset --force --schema=./crates/carbon_repos/prisma/schema.prisma",
    "sentry-cli": "sentry-cli"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.16.0",
    "@sentry/cli": "^2.38.2",
    "@unocss/eslint-config": "^0.65.1",
    "@unocss/transformer-directives": "^0.65.1",
    "better-sqlite3": "^11.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18next": "6.1.1",
    "eslint-plugin-perfectionist": "^4.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-solid": "^0.14.4",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-turbo": "^2.3.3",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "semver": "^7.6.3",
    "turbo": "^2.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.0"
  },
  "engines": {
    "pnpm": ">=9.13.2",
    "node": ">=22.11.0"
  },
  "packageManager": "pnpm@9.13.2"
}
